{{#with model as square}}
	<div class="well well-sm col-sm-1 board-square">
		{{#if wasClicked}}
			<strong>{{number-format nearbyMines}}</strong>
			<i class="fa fa-check text-success"></i>
		{{/if}}
		{{#if square.hasMine}}
			<i class="fa fa-bomb text-danger"></i>
		{{/if}}
		{{#if square.isFlagged}}
			<i class="fa fa-flag text-warning"></i>
		{{/if}}
	</div>
{{/with}}
