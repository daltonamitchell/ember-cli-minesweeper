{{#with model as board}}
	<div class="panel panel-default" oncontextmenu="return false;">
		<div class="panel-body">
			{{#each row in sortedRows}}
				<div class="row">
					{{#each square in row}}
						{{board-square action="gameOver" takeTurn="takeTurn" model=square}}
					{{/each}}
				</div>
			{{/each}}
		</div>
	</div>
{{/with}}